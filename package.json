{
  "name": "boilerplate",
  "productName": "Boilerplate",
  "version": "0.0.1",
  "description": "An Electron boilerplate project",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/appelgriebsch/electron-boilerplate.git"
  },
  "keywords": [
    "electron",
    "boilerplate",
    "angularjs"
  ],
  "author": "Andreas Gerlach <andi@appelgriebsch.org>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/appelgriebsch/electron-boilerplate/issues"
  },
  "homepage": "https://github.com/appelgriebsch/electron-boilerplate#readme",
  "scripts": {
    "start": "electron app/",
    "build": "electron-packager app/ $npm_package_productName --out=pkg --ignore=pkg --prune --asar --all --version=0.32.3",
    "postinstall": "node ./tasks/app_npm_install",
    "app-install": "node ./tasks/app_npm_install"
  },
  "dependencies": {
    "electron-prebuilt": "^0.32.3",
    "grunt": "^0.4.5",
    "grunt-electron": "^2.0.0",
    "load-grunt-tasks": "^3.2.0"
  },
  "optionalDependencies": {
    "grunt-electron-debian-installer": "^0.1.0",
    "grunt-electron-redhat-installer": "^0.1.0",
    "grunt-electron-windows-installer": "^0.1.0"
  }
}
